import java.util.*;
class practice2
{
public static void main(String args[])
{
 Scanner s=new Scanner(System.in);
System.out.println("Enter number a and b:");
 int a=s.nextInt();
 int b=s.nextInt();
System.out.println("1.Addition	2.Subtraction	3.Multiplication	4.Division");
System.out.println("Enter your choice:");
int ch=s.nextInt();
switch(ch)
{
case 1: System.out.println("Addition is "+(a+b));
        break;
case 2: System.out.println("Addition is "+(a-b));
        break;
case 3: System.out.println("Addition is "+(a*b));
        break;
case 4: System.out.println("Addition is "+(a/b));
        break;
default:System.out.println("Choose above options");
}
}
}
